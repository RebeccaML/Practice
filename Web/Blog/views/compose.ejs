<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input type="text" name="postTitle" class="form-control">
    </div>
    <div class="form-group">
        <label>Post</label>
        <textarea name="postBody" class="form-control" rows="10"></textarea>
    </div>
    <button type=submit name="publish" class="btn publish">Publish</button>
</form>
<%- include("partials/footer"); -%>